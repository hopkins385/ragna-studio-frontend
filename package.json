    "build-prod": "vite build --mode production",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/favicon.png"
